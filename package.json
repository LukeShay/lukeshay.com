{
  "name": "lukeshay.com",
  "private": true,
  "homepage": "https://www.lukeshay.com/",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "pnpm run typecheck && pnpm run build.client && pnpm run build.ssr",
    "build.client": "vite build",
    "build.ssr": "vite build --ssr src/entry.cloudflare.tsx",
    "dev": "pnpm run dev.ssr",
    "dev.client": "vite",
    "dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js --force --mode ssr",
    "dev.ssr": "node --inspect node_modules/vite/bin/vite.js --mode ssr",
    "lint": "eslint --ignore-path .gitignore './**/*.{ts,js,cjs,json}'",
    "lint:fix": "pnpm run lint --fix",
    "prettier": "prettier --check --ignore-path .gitignore --ignore-unknown .",
    "prettier:fix": "pnpm run prettier --write",
    "serve": "wrangler pages dev ./dist",
    "start": "pnpm run dev",
    "typecheck": "tsc --incremental --noEmit"
  },
  "dependencies": {
    "@builder.io/qwik": "0.0.100",
    "@builder.io/qwik-city": "0.0.101",
    "@octokit/graphql": "5.0.0"
  },
  "devDependencies": {
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "autoprefixer": "10.4.8",
    "eslint": "8.21.0",
    "eslint-config-get-off-my-lawn": "7.1.0",
    "eslint-plugin-qwik": "0.0.100",
    "node-fetch": "3.2.10",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "prettier-plugin-packagejson": "2.2.18",
    "prettier-plugin-tailwindcss": "0.1.13",
    "tailwindcss": "3.1.8",
    "typescript": "4.7.4",
    "vite": "3.0.5",
    "vite-tsconfig-paths": "3.5.0",
    "wrangler": "beta"
  },
  "engines": {
    "node": "16.x",
    "pnpm": ">=6"
  }
}
