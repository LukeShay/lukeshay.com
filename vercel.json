{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/fonts/inter-var-latin.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/gh",
      "destination": "https://github.com/lukeshay",
      "permanent": true
    },
    {
      "source": "/github",
      "destination": "https://github.com/lukeshay",
      "permanent": true
    }
  ]
}
